{
  "expo": {
    "name": "Taximotour",
    "slug": "Taximotour",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "taximotour",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "navigationBar": {
        "visible": false
      },
      "package": "com.anasnajar.Taximotour"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "dbcc8c7f-b6d7-4984-bf9c-cc3471078280"
      }
    }
  }
}
